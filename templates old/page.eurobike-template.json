            "liquid_content3": "<div class=\"signup-form\" style=\"margin-top:50px;\">\n      {% form 'customer', id: 'home_newsletter_signup' %}\n        {% if form.posted_successfully? %}\n          <div class=\"signup-form__response signup-form__response--success\">\n            <h4>{{ 'newsletter.success' | t }}</h4>\n          </div>\n        {% else %}\n          {%- assign form_errors_id = section.id | append: '-error' -%}\n          {%- render 'form-errors', form: form, id: form_errors_id, class: 'inline-block' -%}\n          <input type=\"hidden\" name=\"contact[tags]\" value=\"Eurobike2025,newsletter\"/>\n          <div>\n            <div class=\"signup-form__inline-container\">\n              <input type=\"email\"\n                placeholder=\"{{ 'newsletter.placeholder' | t }}\"\n                class=\"signup-form__email\"\n                value=\"\"\n                name=\"contact[email]\"\n                aria-label=\"{{ 'newsletter.label' | t }}\"\n                {% if form.errors %}\n                  autofocus\n                  aria-invalid=\"true\"\n                  aria-describedby=\"{{ form_errors_id }}\"\n                {%- endif %}\n                required />\n              <button class=\"btn signup-form__button\" type=\"submit\">\n                {{- 'newsletter.button_label' | t -}}\n              </button>\n            </div>\n          </div>\n        {% endif %}\n      {% endform %}\n    </div>\n<style>\n@media (max-width: 767.98px) {\n #shopify-section-template--23987293290764__custom_two_colum_banner_bTefqW .signup-form__inline-container input {\n        margin-bottom: 0;\n    }\n #shopify-section-template--23987293290764__custom_two_colum_banner_bTefqW .signup-form__inline-container {\n        display: inline-flex;\n        align-items: center;\n        justify-content: center;\n    }\n}\n</style>",
