            "background_padding_right": 0,
            "background_padding_bottom": 0,
            "title_1": "<p>Bikeleasing x HEPHA: Save Up to 40% with Our Easy Bike Leasing!</p>",
            "title_1_de": "<p>Bikeleasing x HEPHA: Dank Bikeleasing bis zu 40% sparen!</p>",
            "title_1_fr": "",
            "heading_color_1": "#ffffff",
            "heading_size_1": 36,
            "subheading_1": "",
