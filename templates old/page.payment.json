        "panel_qmKB3D": {
          "type": "panel",
          "settings": {
            "title": "Is there a minimum amount for financing?",
            "title_de": "Gibt es einen Mindestbetrag für die Finanzierung?",
            "title_fr": "",
            "content_1": "<p>Yes, the total amount in the shopping cart (financing amount) must be at least €99.</p>",
            "content_1_de": "<p>Ja, der Betrag des Warenkorbs (Finanzierungsbetrag) muss mindestens 99 € betragen</p>",
            "content_1_fr": "",
            "content_2": "",
            "content_2_de": "",
            "content_2_fr": "",
            "content_3": "",
            "content_3_de": "",
            "content_3_fr": "",
