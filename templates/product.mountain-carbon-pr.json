          "type": "heading",
          "settings": {
            "title": "Fun Machine.",
            "title_de": "Mehrfach ausgezeichnet.",
            "title_fr": "Plusieurs fois récompensé.",
            "title_fi": "Täyttä hauskuutta!",
            "heading_color": "#ffffff",
