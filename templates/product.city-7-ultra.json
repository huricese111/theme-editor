        "advertisement_cefhch": {
          "type": "advertisement",
          "settings": {
            "ad_link": "shopify://collections/hepha-ebikes",
            "image": "shopify://shop_images/HEPHA-Trekking7_01_grey_7ff138ad-9f41-4981-b752-1bd80fee72c0.jpg",
            "background_color": "#ffffff",
