        "panel_PaQxzy": {
          "type": "panel",
          "settings": {
            "title": "How long is the leasing period?",
            "title_de": "Wie lange ist die Leasingdauer?",
            "title_fr": "",
            "title_fi": "",
            "content_1": "<p>For all of our partners, the leasing period is generally 36 months.</p>",
            "content_1_de": "<p>Bei allen unserer Leasingpartner beträgt die Laufzeit grundsätzlich 36 Monate.</p>",
