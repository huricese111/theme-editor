            "title_1": "<p>Deutsche Dienstrad x HEPHA: Save Up to 40% with Our Easy Bike Leasing!</p>",
            "title_1_de": "<p>Deutsche Dienstrad x HEPHA: Dank Dienstrad-Leasing bis zu 40% sparen!</p>",
            "title_1_fr": "",
            "title_1_fi": "",
            "heading_color_1": "#ffffff",
            "heading_size_1": 36,
            "subheading_1": "",
            "subheading_1_de": "",
            "subheading_1_fr": "",
            "subheading_1_fi": "",
            "subheading_color_1": "#ffffff",
