{% style %}
  :root {
    --header-height: 70px;
    --announcement-height: 45px;
  }
  
  .no-announcement {
    --announcement-height: 0px;
  }
  
  .dynamic-heights {
    --actual-header-height: var(--theme-header-height, 70px);
    --actual-announcement-height: var(--announcement-bar-height, 45px);
  }
{% endstyle %}

<script>
  document.addEventListener('DOMContentLoaded', function() {
    function isDesktop() {
      return window.innerWidth > 768;
    }
    
    function updateHeights() {
      if (!isDesktop()) {
        updateMobileSectionHeight();
        return;
      }
      
      const headerElement = document.querySelector('.section-header, .pageheader');
      const announcementElement = document.querySelector('.announcement-bar, .section-announcement-bar');
      
      let headerHeight = 70; 
      let announcementHeight = 0; 
      
      if (headerElement) {
        headerHeight = headerElement.offsetHeight;
        document.documentElement.style.setProperty('--actual-header-height', headerHeight + 'px');
      }
      
      if (announcementElement && announcementElement.offsetHeight > 0) {
        announcementHeight = announcementElement.offsetHeight;
        document.documentElement.style.setProperty('--actual-announcement-height', announcementHeight + 'px');
      } else {
        document.documentElement.style.setProperty('--actual-announcement-height', '0px');
      }
      
      const totalTopHeight = headerHeight + announcementHeight;
      document.documentElement.style.setProperty('--total-top-height', totalTopHeight + 'px');
    }
    
    function updateMobileSectionHeight() {
      const mapSection = document.querySelector('.map-section');
      const mapSectionLocations = document.querySelector('.map-section__locations');
      const mapSectionMap = document.querySelector('.map-section__map');
      
      if (mapSection && mapSectionLocations && mapSectionMap) {
        setTimeout(() => {
          const locationsHeight = mapSectionLocations.offsetHeight;
          const mapHeight = mapSectionMap.offsetHeight;
          const totalHeight = locationsHeight + mapHeight;
          
          mapSection.style.height = totalHeight + 'px';
          mapSection.style.minHeight = totalHeight + 'px';
        }, 100);
      }
    }
    
    if (isDesktop()) {
      updateHeights();
    } else {
      updateMobileSectionHeight();
    }
    
    window.addEventListener('resize', function() {
      updateHeights();
    });
    
    if (window.Shopify && window.Shopify.designMode) {
      document.addEventListener('shopify:section:load', updateHeights);
      document.addEventListener('shopify:section:reorder', updateHeights);
    }
  });
</script>

