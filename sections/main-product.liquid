    {{ 'ebike.contact-an-expert' | t }}
  </button>
  <button class="btn btn--primary--large notify-me" type="button" onclick="showNotifyMePopup()" style="display: none;">
    {{ 'ebike.notify-me-when-available' | t }}
  </button>
