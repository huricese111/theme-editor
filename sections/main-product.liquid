<script>
  document.addEventListener('DOMContentLoaded', function() {
    const addToCartButton = document.querySelector('.add-to-cart');
    const contactExpertButton = document.querySelector('.contact-expert');
    const notifyMeButton = document.querySelector('.notify-me');
    const contactNotifyButtonsDiv = document.getElementById('contact-notify-buttons');

    // 默认显示按钮容器
    contactNotifyButtonsDiv.style.display = 'block';

    // 创建 MutationObserver 来监视按钮文本变化
    const observer = new MutationObserver((mutationsList) => {
      mutationsList.forEach((mutation) => {
        const buttonText = addToCartButton.innerText.trim().toLowerCase();

        if (mutation.type === 'childList') {
          // 检测具体的文案
          if (buttonText === 'unavailable' || buttonText === 'nicht verfügbar' || buttonText === 'pas disponible') {
            // 如果按钮文本是无库存，显示 Notify Me 按钮
            notifyMeButton.style.display = 'block';
            contactExpertButton.style.display = 'none';
          } else {
            // 否则，显示 Contact Expert 按钮
            contactExpertButton.style.display = 'block';
            notifyMeButton.style.display = 'none';
          }
        }
      });
    });

    // 开始观察按钮文本的变化
    observer.observe(addToCartButton, { childList: true, subtree: true });
  });
</script>

