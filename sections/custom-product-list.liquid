{% schema %}
{
  "name": "Custom Product List",
  "settings": [
    {
      "type": "text",
      "id": "title",
      "label": "Title"
    },
    {
      "type": "radio",
      "id": "product_source",
      "label": "Select Product Source",
      "options": [
        {
          "value": "collection",
          "label": "Load from Collection"
        },
        {
          "value": "manual",
          "label": "Custom Product Selection"
        }
      ],
      "default": "manual"
    },
    {
      "type": "collection",
      "id": "product_collection",
      "label": "Select a Collection",
      "info": "Please select a collection to load products from."
    },
    {
      "type": "product_list",
      "id": "product_list",
      "label": "Product list",
      "info": "Please select multiple products."
    },
    {
      "type": "checkbox",
      "id": "show_description",
      "label": "Show Product Description",
      "default": true
    }, 
    {
      "type": "text",
      "id": "buttonsoldout",
      "label": "Button Sold out Title",
      "default": "Unavailable"      
    },
    {
      "type": "text",
      "id": "buttonsoldout_de",
      "label": "Button Sold out in German",
      "default": "Nicht verfügbar" 
    },
    {
      "type": "text",
      "id": "buttonsoldout_fr",
      "label": "Button Sold out in French",
      "default": "Non disponible" 
    }   
  ],
  "presets": [
    {
      "name": "Custom Product List",
      "category": "Custom"
    }
  ]
}
{% endschema %}



<style>
  .explore-title {
    font-size: 30px;
    margin-bottom: 20px !important;
    text-align: left;
  }

  .benefits-container {
  display: flex;
  height: 80px;
    align-items: center;
}
  .product-content-section {
    max-width: 1200px;
    margin: 0 auto;
    padding: 50px 20px;
  }

  .product-columns {
    display: flex;
    justify-content: flex-start;
    flex-wrap: wrap;
    gap: 1rem;
    padding-top: 20px;
  }

  .product-column {
    width: 32%;
    text-align: left;
    margin-bottom: 30px;
    position: relative; 
  }

  .producttitle {
    color: #000;
  }

  .product-column img {
    width: 100%;
    height: auto;
    aspect-ratio: 1 / 1;
    object-fit: contain;
    border-radius: 5px;
    background: #eeeeee;
    margin-bottom: 10px;
  }

  .product-column h3 {
    font-size: 20px;
  }

  .current-price {
    font-size: 22px;
  }

  .orderbutton {
    display: inline-block;
    padding: 10px 20px;
    background-color: white;
    color: black;
    text-decoration: none;
    border-radius: 50px;
    margin-top: 10px;
    border: 1px solid black;
  }

  .orderbutton:hover {
    background-color: black;
    color: white;
  }

  .customprice {
    margin: 20px 0;
  }

  .products-description {
    margin-top: 20px;
    margin-bottom: 20px;
    background-color: white;
    padding:0;
  }
  
  .discount-image {
    position: absolute;
    top: 10px;
    left: 10px;
    max-width: 120px;  
    max-height: 60px;
    height: 100%; 
    object-fit: contain; 
}

  .discount-label {
    position: absolute;
    top: 10px;
    right: 10px;
    background-color: #FFD200;
    color: black;
    padding: 5px 10px;
    border-radius: 5px;
    font-weight: bold;
    z-index: 10;
  }

  .current-price {
    font-size: 20px;
    margin: 0;
    text-align: left;
    font-weight: 700;
    line-height: 30px;
    color: #333 !important;
}

 .was-price {
    margin-left: 10px;
    text-decoration: line-through;
    color: #bf3737;
    font-size: 20px;
}

  @media (max-width: 768px) {
    .product-column {
      width: 100%;
    }
  }
</style>


